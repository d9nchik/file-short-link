{
  "name": "file-short-link",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^8.6.1",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'  --html sme_result.html",
    "lint": "eslint \"src/**/*.{t,j}s{x,}\"",
    "lint:write": "eslint --debug src/ --fix",
    "format": "prettier --write .",
    "format:stage": "pretty-quick --staged",
    "check-format": "prettier --check .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true npm run test:watch --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "npm run test:watch -- --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.13",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-sonarjs": "^0.7.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.4"
  }
}
